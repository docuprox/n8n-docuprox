{
  "name": "n8n-nodes-docuprox",
  "version": "1.0.0",
  "description": "A n8n Node for processing files and base64 data using the Docuprox API. Easily integrate automated document processing and data extraction into your applications.",
  "keywords": [
  "n8n",
  "n8n-nodes",
  "n8n-community-node-package",
  "docuprox",
  "document-processing",
  "api"
],
  "license": "MIT",
  "homepage": "https://github.com/docuprox/n8n-docuprox",
  "author": {
    "name": "DocuProx",
    "email": "mail@docuprox.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docuprox/n8n-docuprox.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "if exist nodes\\DocuProx\\*.png xcopy /Y nodes\\DocuProx\\*.png dist\\nodes\\DocuProx\\",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/DocuProx/DocuProx.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}